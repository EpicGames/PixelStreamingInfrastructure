    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "sdp": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpicGames/PixelStreamingInfrastructure.git"
